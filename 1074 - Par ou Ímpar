chance = int(input())
for i in range(1, chance+1):
    n = int(input())
    if n == 0:
        print('NULL')
    elif n >= 1 and n % 2 == 0:
       print('EVEN POSITIVE')
    elif n >= 1 and n % 2 == 1:
        print('ODD POSITIVE')
    elif n < 0 and n % 2 == 0:
        print('EVEN NEGATIVE')
    else:
        print('ODD NEGATIVE')


