n = int(input())

for i in range(n):
    x = int(input())
    
    soma = 0

    for i in range(1, x):
        soma += i
        if soma == x:
            print(f'{x} eh perfeito')
            break
        else:
            if soma > x:
                print(f'{x} nao eh perfeito')
                break

n = int(input())
