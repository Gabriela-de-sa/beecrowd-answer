alcool = 0
gasolina = 0
diesel = 0
while True:
    dic = {1: '√Ålcool', 2: 'Gasolina', 
    3: 'Diesel', 4: 'Fim'}
    
    n = int(input())
    if n <= 0 or n > 5:
       continue 
    if n == 4:
        break
    else:
        if n == 1:
            alcool += 1
        elif n == 2:
            gasolina += 1
        elif n == 3:
            diesel += 1

print('MUITO OBRIGADO')
print(f'Alcool: {alcool}')
print(f'Gasolina: {gasolina}')
print(f'Diesel: {diesel}')
